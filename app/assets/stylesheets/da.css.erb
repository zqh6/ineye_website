    text-align: center;
    border: 1px solid #fff;
    padding: 20px 0;
    font-size: 28px;
  }
  .choose p{
    margin-bottom: 20px;
  }
  .chooseWords{
    width: 455px;
    margin-right: 40px;
  }
  .surgeryRelated>div{
    height: 290px;
    margin-bottom: 30px;
    border: 1px solid #33a5b0;
  }
  .landscapeWord{
    font-size: 26px;
    text-align: center;
    padding: 40px 0 40px 10px;
  }
  .listWordCon{
    width: 1100px;
    padding-left: 20px;
  }
  .listWordCon>.listWord{
    width: 800px;
    vertical-align: middle;
    display: inline-block;
    margin-left: 20px;
    line-height: 46px;
    font-size: 16px;
  }
  .listWordCon{
    height: 290px;
    line-height: 290px;
  }
  .listWordCon img{
    vertical-align: middle;
  }
  .shareWord{
    width: 580px;
  }
}
@media screen and (min-width: 320px) and (max-width: 720px){
  body{
    font-size: 0.3rem;
    line-height: 0.48rem;
  }
  .bannerSmall{
    display: block;
  }
  .bannerLarge{
    display: none;
  }
  .main{
    width: 100%;
    padding: 0 0.1rem;
  }
  .leadWord img{
    float: left;
    margin: 0.1rem 0.1rem 0.1rem 0;
  }
  .eyeTle h5{
    font-size: 0.48rem;
  }
  .eyeTle p{
    font-size: 0.36rem;
  }
  .refractiveCon{
    padding: 0;
  }
  .refractiveCon>div{
    width: 100%;
    padding: 0 0.1rem;
    margin-bottom: 0.2rem;
    font-size: 0.28rem;
  }
  .correctRefractive{
    height: auto;
  }
  .correctRefractive img{
    float: left;
    margin-top: 0.1rem;
    width: 2.8rem;
  }
  .correctRefractive>div{
    width: 100%;
    line-height: 0.4rem;
    margin-bottom: 0.6rem;
  }
  .correctRefractive>div>div{
    height: 0.7rem;
    line-height: 0.7rem;
  }
  .correctRefractive>div>p{
    width: auto;
    display: block;
  }
  .choose>p{
    line-height: 1rem;
    font-size: 0.4rem;
    background: #fff;
    margin-bottom: 0.2rem;
    color: #33a5b0;
    text-align: center;
  }
  .choose img{
    width: 100%;
  }
  .landscapeWord{
    width: 0.8rem;
    padding: 0.1rem;
    font-size: 0.36rem;
    text-align: center;
  }
  .surgeryRelated>div{
    border: 1px solid #33a5b0;
    overflow: hidden;
    padding-left: 0.9rem;
    padding-top: 0.1rem;
    margin: 0.3rem 0;
  }
  .listWordCon{
    padding-right: 0.1rem;
  }
  .listWordCon img{
    width: 100%;
  }
  .btnTopCon{
    margin-bottom: 1rem;
  }
  .askBtn{
    right: 0.11rem;
    bottom: 0.1rem;
    width: 98%;
  }
  .photoImg,.shareImgCon{
    width: 100%;
    margin-bottom: 0.2rem;
  }
  .photoImg img,.shareImgCon img{
    width: 100%;
  }
  .photoImg:after{
    border: 0;
  }
  .shareWord p,.shareWordLast p{
    color: #393631;
  }
  .shareWordLast{
    margin: 0;
    line-height: 0.5rem;
  }
  .inlineCenter h5{
    font-size: 0.36rem;
    padding: 0 0.15rem;
  }
  .triangleDiv:after{
    border: 0.25rem solid transparent;
    border-top: 0.4rem solid #33a5b0;
    margin-left: -0.2rem;
    bottom: -0.6rem;
  }
}

