body{
  font-size: 14px;
  line-height: 30px;
  font-family: "Microsoft YaHei", "Arial", "黑体", "宋体", sans-serif;
}
body,p,ul,li,div,h1,h2,h3,h5,h4{
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  list-style: none;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.flow{
  overflow: hidden;
}
.main{
  width: 1200px;
  margin: 0 auto;
}
.bannerSmall{
  display: none;
}
.bannerLarge{
  width: 100%;
  height: 550px;
  background: /*url("../img/yk-banner.jpg")*/ url("<%= asset_path 'yk-banner.jpg' %>") center 0 no-repeat;
}
.bannerLarge img,.bannerSmall img{
  display: block;
}
.marT30{
  margin-top: 30px;
}
.marT20{
  margin-top: 20px;
}
.marB20{
  margin-bottom: 20px;
}
.marB30{
  margin-bottom: 30px;
}
.padT10{
  padding-top: 10px;
}
.padT30{
  padding-top: 30px;
}
.line40{
  line-height: 40px;
}
.whiteBg{
  background: #fff;
}
.blueBg{
  background: #32a4b0;
  color: #fff;
}
.blueBg .eyeTle,.blueBg .eyeTle h5{
  color: #fff;
}
.bannerSmall img{
  width: 100%;
}
.eyeTle{
  text-align: center;
  font-size: 24px;
  color: #999;
  margin: 15px 0;
}
.eyeTle h5{
  font-size: 34px;
  color: #33a5b0;
  line-height: 40px;
  font-weight: normal;
}
.refractiveCon>div{
  width: 455px;
  height: 190px;
  background: #33a5b0;
  color: #fff;
  font-size: 16px;
  line-height: 40px;
  padding: 15px 30px;
  position: relative;
}
.refractiveCon p:first-child{
  font-size: 20px;
}
.refractiveCon{
  overflow: hidden;
  padding: 30px 35px 30px 65px;
}
.refractiveCon>div:before{
  position: absolute;
  content: " ";
  width: 100%;
  height: 100%;
  border: 1px solid #33a5b0;
  left: -30px;
  top: -20px;
}
.correctRefractive{
  height: 220px;
}
.correctRefractive>div{
  width: 560px;
  height: 176px;
  padding-left: 10px;
  line-height: 176px;
  background: #f1f1f1;
  position: relative;
}
.correctRefractive>div>p{
  display: inline-block;
  line-height: 26px;
  vertical-align: middle;
  width: 350px;
}
.correctRefractive>div>div{
  position: absolute;
  background: #33a5b0;
  height: 40px;
  width: 260px;
  margin-left: -130px;
  left: 50%;
  bottom: -20px;
  text-align: center;
  line-height: 40px;
  color: #fff;
  font-size: 20px;
}
.triangleDiv{
  position: relative;
}
.triangleDiv:after{
  content: " ";
  width: 0;
  height: 0;
  border: 19px solid transparent;
  border-top:30px solid #33a5b0;
  margin-left: -26px;
  left: 50%;
  bottom: -48px;
  position: absolute;
}
.correctRefractive img{
  margin-right: 20px;
}
.chooseWords div{
  position: relative;
  margin-bottom: 30px;
  min-height: 60px;
  line-height: 60px;
  padding-left: 20px;
  border: 1px solid #fff;
}
.chooseWords div p{
  line-height:24px;
  vertical-align: middle;
  display: inline-block;
  margin: 6px 0;
}
.chooseWords div:before{
  width: 60px;
  height: 62px;
  line-height: 62px;
  text-align: center;
  background: #fff;
  position: absolute;
  left: -60px;
  top: -1px;
  content: "01";
  font-size: 40px;
  color: #33a5b0;
}
.chooseWords div.tow:before{
  content: "02";
}
.chooseWords div.three:before{
  content: "03";
}
.chooseWords div.four:before{
  content: "04";
}
.chooseWords div.five:before{
  content: "05";
}
.landscapeWord{
  writing-mode: tb-lr;
  -webkit-writing-mode: vertical-lr;
  writing-mode: vertical-lr;
  position: absolute;
  width: 80px;
  top:0;
  left:0;
  height: 100%;
  background: #32a4b0;
  color: #fff;
}
.surgeryRelated>div{
  position: relative;
  padding-left: 80px;
}
.askBtn{
  position: absolute;
  right: 0;
  bottom: 20px;
  width: 100px;
  height: 40px;
  line-height: 40px;
  background: #32a4b0;
  text-align: center;
}
.askBtn a{
  color: #fff;
  display: block;
  height: 100%;
  text-decoration: none;
}
.listWordCon{
  position: relative;
}
.photoImg{
  border:1px solid #fff;
  position: relative;
  padding: 20px;
}
.photoImg:after{
  content: " ";
  left: 0;
  top: 0;
  position: absolute;
  width: 100%;
  height: 100%;
  border: 1px solid #fff;
  transform: rotateZ(10deg);
}
.shareTle{
  line-height: 56px;
  display: inline-block;
  margin: 0 auto 20px;
  padding: 0 25px;
  border: 1px solid #fff;
  font-weight:normal;
  text-align: center;
  font-size: 20px;
}
.inlineCenter{
  text-align: center;
}
.shareImgCon{
  padding: 20px;
  border: 1px solid #fff;
}
.shareWordCon{
  margin: 30px 0;
}
.shareWordLast{
  float: left;
  width: 600px;
  margin-left: 200px;
}
@media screen and (min-width: 720px){
  .leadWord{
    border: 1px solid #b6b6b6;
    line-height: 170px;
    height: 170px;
    padding: 0 20px;
  }
  .leadWord p{
    display: inline-block;
    line-height: 34px;
    width: 1030px;
    margin-left: 20px;
    vertical-align: middle;
  }
  .leadWord img,.correctRefractive img{
