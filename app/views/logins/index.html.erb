<div class="regheader">
    <p class="regpageheader">
      <%= image_tag("reglogo.png") %> <span class="operationtle">用户登录</span>
    </p>
</div>
<%= form_tag sessions_path, method: :post do |f| %>
<div class="loginarea mart150">
    <div>
        手机号：<input type="text" placeholder="请输入注册手机号" class="js-phone-number" maxlength="11">
    </div>
    <div>
        登录密码：<input type="password" class="js-password" placeholder="请输入密码">
    </div>
    <p class="flow">
        <span class="wrong fl warnword">
            用户名或密码不正确
        </span>
        <span class="fr">
        <a href="">注册账号</a><span> | </span><a href="">忘记密码</a>
        </span>
    </p>
</div>
<div class="logsubmit">
  <button type="submit" class="subbtn js-submit">立即登录</button><span style="color: red; margin-left: 20px;" class="js-error-vector"></span>
</div>
<% end %>


<% content_for :script do %>
  <%= javascript_include_tag 'logins/index' %>
<% end %>